        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def check_adult(self):\n",
        "    if self.age>=20:\n",
        "      return True\n",
        "    else:\n",
        "      return False\n",
        "\n",
        "human=Human(\"太郎\", 20)\n",
        "if human.check_adult():\n",
        "  print(\"大人です\")\n",
        "else:\n",
        "  print(\"大人ではありません\")"
      ]
